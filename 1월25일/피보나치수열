package fibonacci;

public class Finum {

	public static void main(String[] args) {

	// 피보나치 수열 - 첫째 항 둘째 항이 1 이며 그 뒤의 모든 항은 바로 앞 두 항의 합이다. ex(1,1,2,3,5,8,13,21 ....
		int arr[] = new int[10];
		
		arr[0]=1;
		arr[1]=1;
		for(int i=2; i<arr.length; i++) {
			arr[i] = arr[i-1]+arr[i-2];
		}
		for(int i=0; i<arr.length; i++) {
			System.out.println(arr[i]);
		}
		
	}

}
